{
	"format_version": "1.10.0",
	"minecraft:entity": {
		"description": {
			"identifier": "custom:dynamite",
			"is_spawnable": true,
			"is_summonable": true,
			"spawn_egg": true,
			"is_experimental": false
		},
		"component_groups": {
			"ignited": {
				"minecraft:damage_sensor": {
					"triggers": {
						"cause": "any",
						"deals_damage": false
					}
				},
				"minecraft:is_ignited": {
					"value": true
				},
				"minecraft:explode": {
					"fuseLength": [1.5, 1.5],
					"fuseLit": true,
					"power": 5
				}
			}
		},
		"components": {
			"minecraft:type_family": { "family": [ "custom", "dynamite" ] },
			"minecraft:physics": {},
			"minecraft:pushable": {
				"is_pushable": false,
				"is_pushable_by_piston": false
			},
			"minecraft:health": {
				"value": 1,
				"max": 1
			},
			"minecraft:collision_box": {
				"width": 1.0,
				"height": 1.0
			},
			"minecraft:movement": {
				"value": 0.0001
			},
			"minecraft:movement.generic": {},
			"minecraft:fall_damage": {
				"value": 0.0
			},
			"minecraft:scale": {
				"value": 1.1
			},
			"minecraft:damage_sensor": {
				"triggers": {
					"cause": "any",
					"deals_damage": false,
					"on_damage": {
						"event": "ignite"
					}
				}
			},
			"minecraft:interact": {
				"on_interact": {
					"filters": {
						"test": "is_family",
						"subject": "other",
						"operator": "==",
						"value": "player"
					},
					"event": "ignite"
				}
			}
		},
		"events": {
			"ignite": {
				"add": { "component_groups": [ "ignited" ] }
			}
		}
	}
}
